{
  "editor.minimap.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "javascript.validate.enable": false,
  "window.zoomLevel": 0,
  "todo-tree.filtering.excludedWorkspaces": ["node_modules", "dist", "build"],
  "todo-tree.general.tagGroups": {
    "FIXME": ["fixme", "FIXME", "FIXIT", "FIX"],
    "TODO": ["TODO", "todo"]
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/build": true,
    "**/CVS": true,
    "**/node_modules": true,
    "/dist/": true,
    "/node_modules/": true
  },
  "yaml.customTags": [
    "!And",
    "!If sequence",
    "!If",
    "!Not",
    "!Not sequence",
    "!Equals sequence",
    "!Or",
    "!FindInMap sequence",
    "!Base64",
    "!Base64 mapping",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!GetAtt",
    "!GetAtt sequence",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Select sequence",
    "!Split sequence",
    "!Join sequence",
    "!And sequence",
    "!Equals",
    "!Or sequence",
    "!FindInMap",
    "!Join",
    "!Sub sequence",
    "!ImportValue sequence",
    "!Split"
  ],
  "yaml.format.enable": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.eol": "\n",
  "editor.tabSize": 2,
  "workbench.editor.highlightModifiedTabs": true,
  "explorer.sortOrder": "type",
  "editor.cursorBlinking": "smooth",
  "files.trimFinalNewlines": true,
  "editor.fontLigatures": true,
  "workbench.fontAliasing": "auto",
  "editor.renderIndentGuides": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    "**/build": true,
    "**/dist": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "extensions.ignoreRecommendations": false,
  "terminal.integrated.rendererType": "dom",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "bookmarks.navigateThroughAllFiles": true,
  "bookmarks.useWorkaroundForFormatters": true,
  "editor.wordWrap": "on",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.fontFamily": "FuraCode NF",
  "todo-tree.tree.showScanModeButton": false,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.renderSideBySide": false,
  "workbench.activityBar.visible": false,
  "breadcrumbs.enabled": false,
  "breadcrumbs.showInterfaces": false,
  "breadcrumbs.showFields": false,
  "prettier.configPath": ".prettierrc",
  "prettier.requireConfig": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "editor.maxTokenizationLineLength": 60000,
  "yaml.format.singleQuote": true,
  "editor.cursorSmoothCaretAnimation": true,
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.renameOnType": true,
  "yaml.validate": false
}
